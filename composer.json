    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
      "psr-4": {
        "Smartbox\\CoreBundle\\": ""
      }
    },
    "require": {
        "php": ">=5.6.0",
        "symfony/symfony": "^2.7.0",
        "symfony/monolog-bundle": "^2.4.0",
        "sensio/distribution-bundle": "~4.0.0",
        "sensio/framework-extra-bundle": "^3.0.0",
        "jms/serializer": "*",
        "doctrine/annotations": "^1.2.0"
    },
    "require-dev": {
        "jms/serializer-bundle": "^2.0.0",
