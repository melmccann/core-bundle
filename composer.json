        "snc/redis-bundle": "^1.1",
        "predis/predis": "^1.0",
        "symfony/phpunit-bridge": "*"
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true
